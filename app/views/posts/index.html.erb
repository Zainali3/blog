<h1> Posts<h1>

<% @posts.each do |post| %>
<h2><%= link_to post.title , post %><h2>
<p><%= post.content %><p>
<p><%= link_to "Edit Post", edit_post_path(post) %></p>
<p><%= link_to "Delete Post", post, :confirm=>"Are you sure", :method => :delete %></p>

<% end %>	
<%= link_to "Add a new Post" , new_post_path %>
